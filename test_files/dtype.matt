dev G1 = NAND;
dev G2 = NAND;
dev G3 = NAND;
dev G4 = NAND;
dev G5 = NAND;
dev G6 = NAND;

dev CLK = CLOCK {
    Period : 2;
}

dev DATA = CLOCK {
    Period : 5;
}

dev G1 {
    I1 : G4;
    I2 : G2;
}

dev G2 {
    I1 : G1;
    I2 : CLK;
}

dev G3 {
    I1 : G2;
    I2 : CLK;
    I3 : G4;
}

dev G4 {
    I1 : G3;
    I2 : DATA;
}

dev G5 {
    I1 : G2;
    I2 : G6;
}

dev G6 {
    I1 : G5;
    I2 : G3;
}

monitor G5 as Q, G6 as QBar;
